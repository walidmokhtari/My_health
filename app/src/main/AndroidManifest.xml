<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="dz.walid.masant">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".main.controllers.MyImc"
            android:parentActivityName=".main.controllers.MainActivity">
        </activity>
        <activity android:name=".main.controllers.MyTemperature"
            android:parentActivityName=".main.controllers.MainActivity">
        </activity>
        <activity android:name=".main.controllers.MyChildsBg"
            android:parentActivityName=".main.controllers.MainActivity">
        </activity>
        <activity android:name=".main.controllers.MyConfinement"
            android:parentActivityName=".main.controllers.MainActivity" >
        </activity>
        <activity android:name=".main.controllers.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>